@misc{chen_measurement_2011,
	title = {Measurement of the transverse diffusion coefficient of charge in liquid xenon},
	url = {http://arxiv.org/abs/1109.3300},
	doi = {10.48550/arXiv.1109.3300},
	abstract = {Liquid xenon (LXe) is a very attractive material as a detection medium for ionization detectors due to its high density, high atomic number, and low energy required to produce electron-ion pairs. Therefore it has been used in several applications, like \{{\textbackslash}gamma\} detection or direct detection of dark matter. Now Subatech is working on the R \& D of LXe Compton telescope for 3\{{\textbackslash}gamma\} medical imaging, which can make precise tridimensional localization of a (\{{\textbackslash}beta\}+, \{{\textbackslash}gamma\}) radioisotope emitter. The diffusion of charge carriers will directly affect the spatial resolution of LXe ionization signal. We will report how we measure the transverse diffusion coefficient for different electric field (0.5 {\textasciitilde} 1.2 kV/cm) by observing the spray of charge carriers on drift length varying until 12cm. With very-low-noise front-end electronics and complete Monte-Carlo simulation of the experiment, the values of transverse diffusion coefficient are measured precisely.},
	urldate = {2022-10-03},
	publisher = {arXiv},
	author = {Chen, W.-T. and Carduner, H. and Cussonneau, J.-P. and Donnard, J. and Duval, S. and Mohamad-Hadi, A.-F. and Lamblin, J. and Lemaire, O. and Ray, P. Le and Morteau, E. and Oger, T. and Scotto-Lavina, L. and Stutzmann, J.-S. and Thers, D.},
