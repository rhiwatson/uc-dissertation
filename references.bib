@article{clark_direct_2020,
	title = {Direct {Detection} {Limits} on {Heavy} {Dark} {Matter}},
	volume = {102},
	issn = {2470-0010, 2470-0029},
	url = {http://arxiv.org/abs/2009.07909},
	doi = {10.1103/PhysRevD.102.123026},
	abstract = {Multiply-interacting massive particles (MIMPs) are heavy ({\textgreater}10{\textasciicircum}10 GeV/c{\textasciicircum}2) dark matter particles that interact strongly with regular matter, but may have evaded detection due to the low number density required to make up the local dark matter halo. These particles could leave track-like signatures in current experiments, similar to lightly-ionizing particles. We show that previously calculated limits from the MAJORANA Demonstrator on the flux of lightly-ionizing particles can be used to exclude MIMP dark matter parameter space up to a mass of 10{\textasciicircum}15 GeV/c{\textasciicircum}2. We also calculate limits from the standard XENON1T analysis in this high-mass regime, properly taking into account flux limitations and multi-scatter effects. Finally, we show that a dedicated MIMP analysis using the XENON1T dark matter search could probe unexplored parameter space up to masses of 10{\textasciicircum}18 GeV/c{\textasciicircum}2.},
	number = {12},
	urldate = {2021-08-30},
	journal = {Physical Review D},
	author = {Clark, Michael and Depoian, Amanda and Elshimy, Bahaa and Kopec, Abigail and Lang, Rafael F. and Qin, Juehang},
	month = dec,
	year = {2020},
	note = {arXiv: 2009.07909},
	keywords = {High Energy Physics - Experiment, High Energy Physics - Phenomenology},
	pages = {123026},
}

@article{digman_not_2019,
	title = {({Not} as) {Big} as a {Barn}: {Upper} {Bounds} on {Dark} {Matter}-{Nucleus} {Cross} {Sections}},
	volume = {100},
	issn = {2470-0010, 2470-0029},
	shorttitle = {({Not} as) {Big} as a {Barn}},
	url = {http://arxiv.org/abs/1907.10618},
	doi = {10.1103/PhysRevD.100.063013},
	abstract = {Critical probes of dark matter come from tests of its elastic scattering with nuclei. The results are typically assumed to be model-independent, meaning that the form of the potential need not be specified and that the cross sections on different nuclear targets can be simply related to the cross section on nucleons. For point-like spin-independent scattering, the assumed scaling relation is \${\textbackslash}sigma\_\{{\textbackslash}chi A\} {\textbackslash}propto A{\textasciicircum}2 {\textbackslash}mu\_A{\textasciicircum}2 {\textbackslash}sigma\_\{{\textbackslash}chi N\}{\textbackslash}propto A{\textasciicircum}4 {\textbackslash}sigma\_\{{\textbackslash}chi N\}\$, where the \$A{\textasciicircum}2\$ comes from coherence and the \${\textbackslash}mu\_A{\textasciicircum}2{\textbackslash}simeq A{\textasciicircum}2 m\_N{\textasciicircum}2\$ from kinematics for \$m\_{\textbackslash}chi{\textbackslash}gg m\_A\$. Here we calculate where model independence ends, i.e., where the cross section becomes so large that it violates its defining assumptions. We show that the assumed scaling relations generically fail for dark matter-nucleus cross sections \${\textbackslash}sigma\_\{{\textbackslash}chi A\} {\textbackslash}sim 10{\textasciicircum}\{-32\}-10{\textasciicircum}\{-27\}{\textbackslash};{\textbackslash}text\{cm\}{\textasciicircum}2\$, significantly below the geometric sizes of nuclei, and well within the regime probed by underground detectors. Last, we show on theoretical grounds, and in light of existing limits on light mediators, that point-like dark matter cannot have \${\textbackslash}sigma\_\{{\textbackslash}chi N\}{\textbackslash}gtrsim10{\textasciicircum}\{-25\}{\textbackslash};{\textbackslash}text\{cm\}{\textasciicircum}2\$, above which many claimed constraints originate from cosmology and astrophysics. The most viable way to have such large cross sections is composite dark matter, which introduces significant additional model dependence through the choice of form factor. All prior limits on dark matter with cross sections \${\textbackslash}sigma\_\{{\textbackslash}chi N\}{\textgreater}10{\textasciicircum}\{-32\}{\textbackslash};{\textbackslash}text\{cm\}{\textasciicircum}2\$ with \$m\_{\textbackslash}chi{\textbackslash}gtrsim 1{\textbackslash};{\textbackslash}text\{GeV\}\$ must therefore be re-evaluated and reinterpreted.},
	number = {6},
	urldate = {2021-08-30},
	journal = {Physical Review D},
	author = {Digman, Matthew C. and Cappiello, Christopher V. and Beacom, John F. and Hirata, Christopher M. and Peter, Annika H. G.},
	month = sep,
	year = {2019},
	note = {arXiv: 1907.10618},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, High Energy Physics - Phenomenology, Nuclear Theory},
	pages = {063013},
}

@article{fischer_influence_2021,
	title = {Influence of the experimental set-up and voltage ramp on the dielectric breakdown strength and breakdown site in borosilicate glass},
