@article{hall_freeze-production_2010,
	title = {Freeze-{In} {Production} of {FIMP} {Dark} {Matter}},
	volume = {2010},
	issn = {1029-8479},
	url = {http://arxiv.org/abs/0911.1120},
	doi = {10.1007/JHEP03(2010)080},
	abstract = {We propose an alternate, calculable mechanism of dark matter genesis, "thermal freeze-in," involving a Feebly Interacting Massive Particle (FIMP) interacting so feebly with the thermal bath that it never attains thermal equilibrium. As with the conventional "thermal freeze-out" production mechanism, the relic abundance reflects a combination of initial thermal distributions together with particle masses and couplings that can be measured in the laboratory or astrophysically. The freeze-in yield is IR dominated by low temperatures near the FIMP mass and is independent of unknown UV physics, such as the reheat temperature after inflation. Moduli and modulinos of string theory compactifications that receive mass from weak-scale supersymmetry breaking provide implementations of the freeze-in mechanism, as do models that employ Dirac neutrino masses or GUT-scale-suppressed interactions. Experimental signals of freeze-in and FIMPs can be spectacular, including the production of new metastable coloured or charged particles at the LHC as well as the alteration of big bang nucleosynthesis.},
	number = {3},
	urldate = {2022-08-31},
	journal = {Journal of High Energy Physics},
	author = {Hall, Lawrence J. and Jedamzik, Karsten and March-Russell, John and West, Stephen M.},
	month = mar,
	year = {2010},
	note = {arXiv:0911.1120 [astro-ph, physics:hep-ph]},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, High Energy Physics - Phenomenology},
	pages = {80},
}

@misc{carney_snowmass2021_2022,
	title = {Snowmass2021 {Cosmic} {Frontier} {White} {Paper}: {Ultraheavy} particle dark matter},
	shorttitle = {Snowmass2021 {Cosmic} {Frontier} {White} {Paper}},
	url = {http://arxiv.org/abs/2203.06508},
	abstract = {We outline the unique opportunities and challenges in the search for "ultraheavy" dark matter candidates with masses between roughly \$10{\textasciitilde}\{{\textbackslash}rm TeV\}\$ and the Planck scale \$m\_\{{\textbackslash}rm pl\} {\textbackslash}approx 10{\textasciicircum}\{16\}{\textasciitilde}\{{\textbackslash}rm TeV\}\$. This mass range presents a wide and relatively unexplored dark matter parameter space, with a rich space of possible models and cosmic histories. We emphasize that both current detectors and new, targeted search techniques, via both direct and indirect detection, are poised to contribute to searches for ultraheavy particle dark matter in the coming decade. We highlight the need for new developments in this space, including new analyses of current and imminent direct and indirect experiments targeting ultraheavy dark matter and development of new, ultra-sensitive detector technologies like next-generation liquid noble detectors, neutrino experiments, and specialized quantum sensing techniques.},
	language = {en},
	urldate = {2022-08-31},
	publisher = {arXiv},
	author = {Carney, Daniel and Raj, Nirmal and Bai, Yang and Berger, Joshua and Blanco, Carlos and Bramante, Joseph and Cappiello, Christopher and Dutra, Maíra and Ebadi, Reza and Engel, Kristi and Kolb, Edward and Harding, J. Patrick and Kumar, Jason and Krnjaic, Gordan and Lang, Rafael F. and Leane, Rebecca K. and Lehmann, Benjamin V. and Li, Shengchao and Long, Andrew J. and Mohlabeng, Gopolang and Olcina, Ibles and Pueschel, Elisa and Rodd, Nicholas L. and Rott, Carsten and Sengupta, Dipan and Shakya, Bibhushan and Walsworth, Ronald L. and Westerdale, Shawn},
	month = mar,
	year = {2022},
	note = {arXiv:2203.06508 [astro-ph, physics:hep-ex, physics:hep-ph]},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, High Energy Physics - Experiment, High Energy Physics - Phenomenology},
}

@article{kolb_superheavy_2017,
	title = {Superheavy dark matter through {Higgs} portal operators},
	volume = {96},
	issn = {2470-0010, 2470-0029},
	url = {http://arxiv.org/abs/1708.04293},
	doi = {10.1103/PhysRevD.96.103540},
	abstract = {The WIMPzilla hypothesis is that the dark matter is a super-weakly-interacting and super-heavy particle. Conventionally, the WIMPzilla abundance is set by gravitational particle production during or at the end of inflation. In this study we allow the WIMPzilla to interact directly with Standard Model fields through the Higgs portal, and we calculate the thermal production (freeze-in) of WIMPzilla dark matter from the annihilation of Higgs boson pairs in the plasma. The two particle-physics model parameters are the WIMPzilla mass and the Higgs-WIMPzilla coupling. The two cosmological parameters are the reheating temperature and the expansion rate of the universe at the end of inflation. We delineate the regions of parameters space where either gravitational or thermal production is dominant, and within those regions we identify the parameters that predict the observed dark matter relic abundance. Allowing for thermal production opens up the parameter space, even for Planck-suppressed Higgs-WIMPzilla interactions.},
	number = {10},
	urldate = {2022-08-29},
	journal = {Physical Review D},
	author = {Kolb, Rocky W. and Long, Andrew J.},
	month = nov,
	year = {2017},
	note = {arXiv:1708.04293 [astro-ph, physics:hep-ph]},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, High Energy Physics - Phenomenology},
	pages = {103540},
}

@article{gresham_astrophysical_2018,
	title = {Astrophysical {Signatures} of {Asymmetric} {Dark} {Matter} {Bound} {States}},
	volume = {98},
	issn = {2470-0010, 2470-0029},
	url = {http://arxiv.org/abs/1805.04512},
	doi = {10.1103/PhysRevD.98.096001},
	abstract = {Nuggets---very large stable bound objects arising in the presence of a sufficiently attractive and long-range force and in the absence of a dark Coulomb force---are a smoking gun signature for Asymmetric Dark Matter (ADM). The cosmology of ADM nuggets is both generic and unique: nuggets feature highly exothermic fusion processes, which can impact the shape of the core in galaxies, as well as give rise to rare dark star formation. We find, considering the properties of nuggets in a generic extended nuclear model with both attractive and repulsive forces, that self-interaction constraints place an upper bound on nugget masses at the freeze-out of synthesis in the ballpark of \$M\_\{{\textbackslash}rm fo\} {\textbackslash}lesssim 10{\textasciicircum}\{16\}\$ GeV. We also show that indirect detection strongly constrains models where the scalar mediator binding the nuggets mixes with the Higgs.},
	number = {9},
	urldate = {2022-08-22},
	journal = {Physical Review D},
	author = {Gresham, Moira I. and Lou, Hou Keong and Zurek, Kathryn M.},
	month = nov,
	year = {2018},
	note = {arXiv:1805.04512 [astro-ph, physics:hep-ph]},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena, High Energy Physics - Phenomenology},
	pages = {096001},
}

@article{petraki_review_2013,
	title = {Review of asymmetric dark matter},
	volume = {28},
	issn = {0217-751X, 1793-656X},
