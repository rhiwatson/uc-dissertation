	url = {http://arxiv.org/abs/1903.05429},
	doi = {10.1103/PhysRevD.99.103016},
	abstract = {In 15 years of data taking the Pierre Auger Observatory has observed no events beyond \$10{\textasciicircum}\{11.3\}\$ GeV. This null result translates into an upper bound on the flux of ultrahigh-energy cosmic rays implying \$J ({\textgreater} 10{\textasciicircum}\{11.3\}\$ GeV) \${\textless} 3.6 {\textbackslash}times 10{\textasciicircum}\{-5\}\$ km\${\textasciicircum}\{-2\}\$ sr\${\textasciicircum}\{-1\}\$ yr\${\textasciicircum}\{-1\}\$, at the 90{\textbackslash}\%C.L. We interpret this bound as a constraint on extreme-energy photons originating in the decay super-heavy dark matter (SHDM) particles clustered in the Galactic halo. Armed with this constraint we derive the strongest lower limit on the lifetime of hadronically decaying SHDM particles with masses in the range, \$10{\textasciicircum}\{14\} {\textless} M\_X/ \$ GeV \${\textless} 10{\textasciicircum}\{15\}\$. We also explore the capability of future NASA's POEMMA mission to search for SHDM signals.},
	number = {10},
	urldate = {2022-09-13},
	journal = {Physical Review D},
	author = {Alcantara, Esteban and Anchordoqui, Luis A. and Soriano, Jorge F.},
	month = may,
