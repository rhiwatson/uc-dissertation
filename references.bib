@misc{berzak_paschens_2006,
	title = {Paschen's {Law} in {Air} and {Noble} {Gases}},
	url = {http://www-eng.lbl.gov/~shuman/XENON/REFERENCES&OTHER_MISC/paschen_report.pdf},
	abstract = {Paschen curves for air, helum, neon, and xenon are obtained by measuring the breakdown voltage of gas within a Pyrex vacuum vessel with two planar, stainless steel electrodes. Data is taken at various pressuresand electrode separations and fit ot a Paschen curve. The constants A' and B are extracted both from the fit and from examining the minimum of the experimental curve. The trends in the shapes of the curves are examined and explained based on the electrochemical properties of the various gases. Despite showing similar trends, our measurements differed from the reported values by as many as five standard deviations. Future experiments using more precise, automated voltage adjustment are proposed to correct these deviations.},
	author = {Berzak, L. F. and Dorfman, S. E. and Smith, S. P.},
	month = apr,
	year = {2006},
}

@article{bhattacharya_measurement_1976,
	title = {Measurement of breakdown potentials and {Townsend} ionization coefficients for the {Penning} mixtures of neon and xenon},
	doi = {10.1103/PHYSREVA.13.1219},
	abstract = {The Townsend primary (\${\textbackslash}ensuremath\{{\textbackslash}alpha\}\$) and secondary (\${\textbackslash}ensuremath\{{\textbackslash}gamma\}\$) coefficients and Paschen curves for neon, xenon, and their mixtures are reported. The primary coefficient was determined by measuring the variation in the luminous flux in a self-sustained Townsend discharge between two parallel-plate nickel electrodes spaced 21.5 mm apart. The values of the reduced primary coefficient, \${\textbackslash}frac\{{\textbackslash}ensuremath\{{\textbackslash}alpha\}\}\{\{p\}\_\{0\}\}\$, for Ne and Xe are about 10-20\% lower than published values obtained by the classical method of Townsend. The \${\textbackslash}frac\{{\textbackslash}ensuremath\{{\textbackslash}alpha\}\}\{\{p\}\_\{0\}\}\$ values obtained for Ne agree with previous work using the same luminous-flux method. The ionization efficiency function \${\textbackslash}ensuremath\{{\textbackslash}eta\}\$ \${\textbackslash}ensuremath\{{\textbackslash}eta\}={\textbackslash}frac\{{\textbackslash}ensuremath\{{\textbackslash}alpha\}\}\{E\}\$, where \$E\$ is the electric field) has a maximum value for Ne and 0.01\% Xe. Also, the value of \${\textbackslash}ensuremath\{{\textbackslash}eta\}\$ for this gas mixture is larger than that for Ne + 0.1\% Ar, especially at small values of \${\textbackslash}frac\{E\}\{\{p\}\_\{0\}\}\$ (1-10 V \$\{{\textbackslash}mathrm\{cm\}\}{\textasciicircum}\{{\textbackslash}ensuremath\{-\}1\}\$ \$\{{\textbackslash}mathrm\{Torr\}\}{\textasciicircum}\{{\textbackslash}ensuremath\{-\}1\}\$).},
	author = {Bhattacharya, A.},
