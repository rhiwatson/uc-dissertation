	author = {EDELWEISS Collaboration and Arnaud, Q. and Armengaud, E. and Augier, C. and Benoît, A. and Bergé, L. and Billard, J. and Broniatowski, A. and Camus, P. and Cazes, A. and Chapellier, M. and Charlieux, F. and De Jésus, M. and Dumoulin, L. and Eitel, K. and Elkhoury, E. and Fillipini, J.-B. and Filosofov, D. and Gascon, J. and Giuliani, A. and Gros, M. and Jin, Y. and Juillard, A. and Kleifges, M. and Lattaud, H. and Marnieros, S. and Misiak, D. and Navick, X.-F. and Nones, C. and Olivieri, E. and Oriol, C. and Pari, P. and Paul, B. and Poda, D. and Rozov, S. and Salagnac, T. and Sanglard, V. and Siebenborn, B. and Vagneron, L. and Weber, M. and Yakushev, E. and Zolotarova, A.},
	month = oct,
