	author = {Angloher, G. and Bento, A. and Bucci, C. and Canonica, L. and Defay, X. and Erb, A. and von Feilitzsch, F. and Iachellini, N. Ferreiro and Gorla, P. and Gütlein, A. and Hauff, D. and Jochum, J. and Kiefer, M. and Kluck, H. and Kraus, H. and Lanfranchi, J. C. and Loebell, J. and Münster, A. and Pagliarone, C. and Petricca, F. and Potzel, W. and Pröbst, F. and Reindl, F. and Schäffner, K. and Schieck, J. and Schönert, S. and Seidel, W. and Stodolsky, L. and Strandhagen, C. and Strauss, R. and Tanzke, A. and Trinh Thi, H. H. and Türkoğlu, C. and Uffinger, M. and Ulrich, A. and Usherov, I. and Wawoczny, S. and Willers, M. and Wüstrich, M. and Zöller, A.},
