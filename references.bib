@article{supercdms_collaboration_constraints_2020,
	title = {Constraints on low-mass, relic dark matter candidates from a surface-operated {SuperCDMS} single-charge sensitive detector},
	volume = {102},
	issn = {2470-0010, 2470-0029},
	url = {http://arxiv.org/abs/2005.14067},
	doi = {10.1103/PhysRevD.102.091101},
	abstract = {This article presents an analysis and the resulting limits on light dark matter inelastically scattering off of electrons, and on dark photon and axion-like particle absorption, using a second-generation SuperCDMS high-voltage eV-resolution detector. The 0.93 gram Si detector achieved a 3 eV phonon energy resolution; for a detector bias of 100 V, this corresponds to a charge resolution of 3\% of a single electron-hole pair. The energy spectrum is reported from a blind analysis with 1.2 gram-days of exposure acquired in an above-ground laboratory. With charge carrier trapping and impact ionization effects incorporated into the dark matter signal models, the dark matter-electron cross section \${\textbackslash}bar\{{\textbackslash}sigma\}\_\{e\}\$ is constrained for dark matter masses from 0.5--\$10{\textasciicircum}\{4\} \$MeV\$/c{\textasciicircum}\{2\}\$; in the mass range from 1.2--50 eV\$/c{\textasciicircum}\{2\}\$ the dark photon kinetic mixing parameter \${\textbackslash}varepsilon\$ and the axioelectric coupling constant \$g\_\{ae\}\$ are constrained. The minimum 90\% confidence-level upper limits within the above mentioned mass ranges are \${\textbackslash}bar\{{\textbackslash}sigma\}\_\{e\}{\textbackslash},={\textbackslash},8.7{\textbackslash}times10{\textasciicircum}\{-34\}\$ cm\${\textasciicircum}\{2\}\$, \${\textbackslash}varepsilon{\textbackslash},={\textbackslash},3.3{\textbackslash}times10{\textasciicircum}\{-14\}\$, and \$g\_\{ae\}{\textbackslash},={\textbackslash},1.0{\textbackslash}times10{\textasciicircum}\{-9\}\$.},
	number = {9},
